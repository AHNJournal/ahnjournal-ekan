{
  "name": "eightyoptions/ekan",
  "description": "Providing an upgrade path for DKAN sites moving to Drupal 9+",
  "type": "drupal-profile",
  "require": {
    "ext-json": "*",
    "drupal/core": "^9.0",
    "cweagans/composer-patches": "^1.7",
    "drupal/facets": "^2.0",
    "drupal/field_group": "^3.1",
    "drupal/geofield": "^1.15",
    "drupal/group": "^1.3",
    "drupal/key_value_field": "^1.2",
    "drupal/leaflet": "^2.1",
    "drupal/migrate_tools": "^5.0",
    "drupal/radix": "^4.10",
    "drupal/role_delegation": "^1.1",
    "drupal/search_api": "^1.17",
    "drupal/twig_tweak": "^2.6",
    "drupal/views_flipped_table": "^1.4",
    "drupal/autocomplete_deluxe": "^2.0",
    "drupal/update_helper": "^2.0",
    "drupal/pathauto": "^1.8",
    "drupal/allowed_formats": "^1.3",
    "drupal/paragraphs": "^1.12",
    "drupal/components": "^2.4"
  },
  "minimum-stability": "dev",
  "extra": {
  "patches": {
      "drupal/migrate_tools": {
        "Run drush imports as user=1": "https://www.drupal.org/files/issues/2020-05-06/3134245.patch"
      }
    }
  }
}
